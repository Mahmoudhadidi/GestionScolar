    <stylesheets>
        <URL value="@gestionclasse.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableClasse" layoutX="-4.0" layoutY="40.0" prefHeight="235.0" prefWidth="388.0">
        <columns>
          <TableColumn fx:id="classe" prefWidth="75.0" text="Classe" />
          <TableColumn fx:id="capacite" prefWidth="75.0" text="Capacité" />
            <TableColumn fx:id="spécialiste" prefWidth="75.0" text="Spécialiste" />
            <TableColumn fx:id="action" minWidth="15.0" prefWidth="90.0" text="Action" />
        </columns>
      </TableView>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" text="Button" />
        </items>
      </ToolBar>
   </children>
</AnchorPane>
